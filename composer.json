{
	"name": "juniwalk/orm",
	"description": "Nice to have tools for doctrine/orm in nette framework.",
	"type": "library",
	"keywords": ["nette","nettefw","doctrine","orm"],
	"license": "MIT",

	"authors": [{
		"name": "Martin ProchÃ¡zka",
		"email": "juniwalk@outlook.cz",
		"role": "Developer"
	}],

	"autoload": {
		"psr-4": {"JuniWalk\\ORM\\": "src/"}
	},

	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},

	"require": {
		"php": ">=8.1",
		"juniwalk/utils": ">=2.0",
		"nettrine/migrations": "^0.9",
		"nettrine/orm": "^0.8",
		"ramsey/uuid": "^4.0"
	},

	"require-dev": {
		"nette/forms": "^3.0",
		"nette/tester": "^2.5",
		"phpstan/extension-installer": "^1.0",
		"phpstan/phpstan": "^1.0",
		"phpstan/phpstan-nette": "^1.0",
		"staabm/phpstan-todo-by": "^0.1",
		"ublaboo/datagrid": "^6.10"
	},

	"suggest": {
		"nette/forms": "Needed for entity reference from form input."
	}
}
